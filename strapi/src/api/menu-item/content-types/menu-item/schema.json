{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "singularName": "menu-item",
    "pluralName": "menu-items",
    "displayName": "Menu Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "seasonal": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "availableUntil": {
      "type": "date",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "seasonal"
            },
            true
          ]
        }
      },
      "required": true
    },
    "ingredients": {
      "type": "customField",
      "customField": "plugin::strapi-plugin-sortable-list.sortable-list",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "school": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Amador Valley High School",
        "Foothill High School",
        "Village High School"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
